@prefix : <http://gmo#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://gmo#> .

<http://gmo> rdf:type owl:Ontology ;
              owl:imports <http://www.opengis.net/ont/geosparql> ,
                          <http://www.w3.org/2006/time> ;
              rdfs:label "GeoLink Modular Ontology" .

#################################################################
#    Annotation properties
#################################################################

###  http://gmo#DASE_RULE
:DASE_RULE rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://gmo#agentRoleIn
:agentRoleIn rdf:type owl:ObjectProperty ;
             owl:inverseOf :providesAgentRole ;
             rdfs:domain :AgentRole ;
             rdfs:comment "agentRoleIn connects a role to the thing thing in which the role is performed by the agent." ;
             rdfs:label "agentRoleIn" .


###  http://gmo#associatedWithProgram
:associatedWithProgram rdf:type owl:ObjectProperty ;
                       rdfs:range :Program ;
                       rdfs:label "associatedWithProgram" .


###  http://gmo#atPort
:atPort rdf:type owl:ObjectProperty ;
        owl:propertyChainAxiom _:genid4 ;
        rdfs:comment "atPort connects a fix to an instance of Port. It is used when the fix corresponds to a port stop in the beginning, middle, or end of a cruise." ;
        rdfs:label "atPort" .

_:genid4 rdf:type rdf:List ;
          rdf:first :hasPosition ;
          rdf:rest ( [ owl:inverseOf :hasSpatialFootprint
                     ]
                     :r_Port
                   ) .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :atPort ;
   owl:annotatedProperty owl:propertyChainAxiom ;
   owl:annotatedTarget _:genid4 ;
   :DASE_RULE "R2___trajectory:hasPosition(?x,?y) ^ place:hasSpatialFootprint(?z,?y) ^ :Port(?z) -> :atPort(?x,?z)___If position of a fix corresponds to a (spatial footprint of a) port, then that fix is at that port.___http://gmo#freshObjectProperty1"
 ] .


###  http://gmo#atTime
:atTime rdf:type owl:ObjectProperty ;
        rdfs:comment "Provides temporal information of a fix." ;
        rdfs:label "atTime" .


###  http://gmo#derivedUsing
:derivedUsing rdf:type owl:ObjectProperty ;
              rdfs:domain :PhysicalSample ;
              rdfs:range :SamplingProcess .


###  http://gmo#describedBy
:describedBy rdf:type owl:ObjectProperty ;
             rdfs:range :InformationObject ;
             rdfs:label "describedBy" .


###  http://gmo#endsAtFix
:endsAtFix rdf:type owl:ObjectProperty ;
           owl:propertyChainAxiom ( :startsFromFix
                                    :hasNextFix
                                  ) ;
           rdfs:comment "Provides the fix where the segment ends." ;
           rdfs:label "endsAtFix" .


###  http://gmo#endsAtTime
:endsAtTime rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :occursAtTime ;
            rdfs:label "endsAtTime" .


###  http://gmo#endsOnDate
:endsOnDate rdf:type owl:ObjectProperty ;
            rdfs:label "endsOnDate" .


###  http://gmo#fundedBy
:fundedBy rdf:type owl:ObjectProperty ;
          rdfs:range :FundingAward .


###  http://gmo#hasAgentRoleType
:hasAgentRoleType rdf:type owl:ObjectProperty ;
                  rdfs:label "hasAgentRoleType" .


###  http://gmo#hasAlternativeIdentifier
:hasAlternativeIdentifier rdf:type owl:ObjectProperty ;
                          rdfs:label "hasAlternativeIdentifier" .


###  http://gmo#hasAttribute
:hasAttribute rdf:type owl:ObjectProperty ;
              rdfs:comment "Provides an attribute of a fix or a segment." ;
              rdfs:label "hasAttribute" .


###  http://gmo#hasAwardAmount
:hasAwardAmount rdf:type owl:ObjectProperty .


###  http://gmo#hasCurrencyCode
:hasCurrencyCode rdf:type owl:ObjectProperty .


###  http://gmo#hasDataSource
:hasDataSource rdf:type owl:ObjectProperty ;
               rdfs:comment "Provides the source (e.g., a sensor, a device, etc.) from which information about the fix was obtained." ;
               rdfs:label "hasDataSource" .


###  http://gmo#hasFix
:hasFix rdf:type owl:ObjectProperty ;
        owl:propertyChainAxiom ( :hasFix
                                 :hasNextFix
                               ) ;
        rdfs:comment "Provides a fix for the trajectory." ;
        rdfs:label "hasFix" .


###  http://gmo#hasInputFeature
:hasInputFeature rdf:type owl:ObjectProperty .


###  http://gmo#hasNextFix
:hasNextFix rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ;
            rdfs:comment "Provides the next fix after this fix in the sequence." ;
            rdfs:label "hasNextFix" .


###  http://gmo#hasPersonName
:hasPersonName rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :hasPersonalInfoItem ;
               rdfs:label "hasPersonName" .


###  http://gmo#hasPersonalInfoItem
:hasPersonalInfoItem rdf:type owl:ObjectProperty ;
                     rdfs:label "hasPersonalInfoItem" .


###  http://gmo#hasPosition
:hasPosition rdf:type owl:ObjectProperty ;
             rdfs:comment "Provides a geographical location of a fix." ;
             rdfs:label "hasPosition" .


###  http://gmo#hasPrimaryIdentifier
:hasPrimaryIdentifier rdf:type owl:ObjectProperty ;
                      rdfs:label "hasPrimaryIdentifier" .


###  http://gmo#hasProperty
:hasProperty rdf:type owl:ObjectProperty ;
             rdfs:range :Property ;
             rdfs:label "hasProperty" .


###  http://gmo#hasRelatedCruise
:hasRelatedCruise rdf:type owl:ObjectProperty ;
                  rdfs:label "hasRelatedCruise" .


###  http://gmo#hasSegment
:hasSegment rdf:type owl:ObjectProperty ;
