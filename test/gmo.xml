:CuratorRole rdf:type owl:Class ;
             rdfs:subClassOf :AgentRole ;
             rdfs:label "CuratorRole" .


###  http://gmo#CurrencyCode
:CurrencyCode rdf:type owl:Class ;
              rdfs:label "CurrencyCode" .


###  http://gmo#DataManagerRole
:DataManagerRole rdf:type owl:Class ;
                 rdfs:subClassOf :AgentRole ;
                 rdfs:label "DataManagerRole" .


###  http://gmo#EditorRole
:EditorRole rdf:type owl:Class ;
            rdfs:subClassOf :AgentRole ;
            rdfs:label "EditorRole" .


###  http://gmo#EndingFix
:EndingFix rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( :Fix
                                                      [ rdf:type owl:Class ;
                                                        owl:complementOf [ rdf:type owl:Restriction ;
                                                                           owl:onProperty :hasNextFix ;
                                                                           owl:someValuesFrom :Fix
                                                                         ]
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           rdfs:comment "An instance of EndingFix represents the final fix in the specification of a trajectory" ;
           rdfs:label "EndingFix" .


###  http://gmo#Event
:Event rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :occursAtPlace ;
                         owl:someValuesFrom :Place
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :occursAtTime ;
                         owl:someValuesFrom <http://www.w3.org/2006/time#TemporalEntity>
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :endsAtTime ;
                         owl:allValuesFrom <http://www.w3.org/2006/time#Instant>
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :occursAtPlace ;
                         owl:allValuesFrom :Place
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :occursAtTime ;
                         owl:allValuesFrom <http://www.w3.org/2006/time#TemporalEntity>
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :providesAgentRole ;
                         owl:allValuesFrom :AgentRole
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :startsAtTime ;
                         owl:allValuesFrom <http://www.w3.org/2006/time#Instant>
                       ] ;
       rdfs:label "Event" .


###  http://gmo#Feature
:Feature rdf:type owl:Class .


###  http://gmo#Fix
:Fix rdf:type owl:Class ;
     rdfs:subClassOf [ rdf:type owl:Restriction ;
                       owl:onProperty :atTime ;
                       owl:someValuesFrom <http://www.w3.org/2006/time#TemporalEntity>
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :hasPosition ;
                       owl:someValuesFrom :Position
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty [ owl:inverseOf :hasFix
                                      ] ;
                       owl:someValuesFrom :Trajectory
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :atTime ;
                       owl:allValuesFrom <http://www.w3.org/2006/time#TemporalEntity>
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :hasAttribute ;
                       owl:allValuesFrom :Attribute
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :hasDataSource ;
                       owl:allValuesFrom :Source
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :hasNextFix ;
                       owl:allValuesFrom :Fix
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :hasPosition ;
                       owl:allValuesFrom :Position
                     ] ;
     rdfs:comment "A fix represents a spatiotemporal point in the specification of a trajectory.  A fix has a location and a time stamp, and always belongs to one particular trajectory. Also, a fix cannot be followed by more than one other fix, and cannot follow itself. This gives a linear structure in the ordering of the fixes." ;
     rdfs:label "Fix" .


