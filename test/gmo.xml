             owl:inverseOf :performsAgentRole ;
             rdfs:comment "performedBy connects  a role to the agent that performs it." ;
             rdfs:label "performedBy" .


###  http://gmo#performsAgentRole
:performsAgentRole rdf:type owl:ObjectProperty ;
                   rdfs:label "performsAgentRole" .


###  http://gmo#providesAgentRole
:providesAgentRole rdf:type owl:ObjectProperty ;
                   rdfs:range :AgentRole ;
                   rdfs:comment "providesAgentRole is the inverse of isAgentRoleIn." ;
                   rdfs:label "providesAgentRole" .


###  http://gmo#r_Port
:r_Port rdf:type owl:ObjectProperty ;
        rdfs:comment "The class Port typecasted as an object property. Logically, r_Port(x,x) <-> Port(x)" ;
        rdfs:label "r_Port" .


###  http://gmo#realizationAbout
:realizationAbout rdf:type owl:ObjectProperty ;
                  rdfs:domain :InformationRealization ;
                  owl:propertyChainAxiom ( [ owl:inverseOf :realizedBy
                                           ]
                                           [ owl:inverseOf :describedBy
                                           ]
                                         ) ;
                  rdfs:label "realizationAbout" .


###  http://gmo#realizedBy
:realizedBy rdf:type owl:ObjectProperty ;
            rdfs:label "realizedBy" .


###  http://gmo#startsAtTime
:startsAtTime rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :occursAtTime ;
              rdfs:label "startsAtTime" .


###  http://gmo#startsFromFix
:startsFromFix rdf:type owl:ObjectProperty ,
                        owl:FunctionalProperty ;
               rdfs:comment "Provides the fix from which the segment starts." ;
               rdfs:label "startsFromFix" .


###  http://gmo#startsOnDate
:startsOnDate rdf:type owl:ObjectProperty ;
              rdfs:label "startsOnDate" .


###  http://gmo#traveledBy
:traveledBy rdf:type owl:ObjectProperty ;
            owl:propertyChainAxiom ( [ owl:inverseOf :hasTrajectory
                                     ]
                                     :undertakenBy
                                   ) ;
            rdfs:comment "If a cruise is undertaken by a vessel, then its trajectory is traveled by the vessel." ;
            rdfs:label "traveledBy" .


###  http://gmo#traversedBy
:traversedBy rdf:type owl:ObjectProperty ;
             owl:propertyChainAxiom _:genid23 ;
             rdfs:comment "Provides a moving object that traverses the segment." ;
             rdfs:label "traversedBy" .

_:genid23 rdf:type rdf:List ;
           rdf:first [ owl:inverseOf :hasSegment
                     ] ;
           rdf:rest ( :traveledBy
                    ) .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :traversedBy ;
   owl:annotatedProperty owl:propertyChainAxiom ;
   owl:annotatedTarget _:genid23 ;
   rdfs:comment """This axiom is equivalent to the rule: 
:traveledBy(?x,?y) ^ trajectory:hasSegment(?x,?z) -> trajectory:traversedBy(?z,?y)"""
 ] .


###  http://gmo#undertakenBy
:undertakenBy rdf:type owl:ObjectProperty ;
              rdfs:comment "This connects a cruise to the vessel on which the cruise is conducted." ;
              rdfs:label "undertakenBy" .


