<http://gbo> rdf:type owl:Ontology ;
              <http://purl.org/dc/terms/description> <http://schema.geolink.org/images/gbo-overview.svg> ;
              rdfs:comment "A base ontology providing a conceptual view into the GeoLink patterns. For data providers, this is the default view of the patterns." ;
              rdfs:label "GeoLink base ontology." .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/description
<http://purl.org/dc/terms/description> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.opengis.net/ont/geosparql#wktLiteral
<http://www.opengis.net/ont/geosparql#wktLiteral> rdf:type rdfs:Datatype ;
                                                  rdfs:comment "Enables the use of Well Known Text strings" ;
                                                  rdfs:label "geosparql:wktLiteral" .


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#HTML
rdf:HTML rdf:type rdfs:Datatype ;
         rdfs:comment "Enables the use of HTML strings" ;
         rdfs:label "rdf:HTML" .


###  http://www.w3.org/2001/XMLSchema#gYear
xsd:gYear rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gYearMonth
xsd:gYearMonth rdf:type rdfs:Datatype .


